<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/colorPrimaryDark"/>

            <size
                android:width="32dp"
                android:height="32dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#FFFF"/>

            <size
                android:width="32dp"
                android:height="32dp"/>
        </shape>
    </item>
</layer-list>
